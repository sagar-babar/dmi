	output_include_time true

	buffer_type        file
	buffer_path        /logs/td-agent/buffer/dsp.dls.registration.events.api.buffer
	buffer_chunk_limit 4m
	buffer_queue_limit 204800

	flush_interval     10s
	retry_wait         10s
	retry_limit        50
	num_threads        4
</match>

