upstream  aacmi_core {
       	<% @aacmi_tomcat_servers.each do |aacmitomcatservers| -%>
server  <%=aacmitomcatservers-%>:8080  weight=1  max_fails=2  fail_timeout=5s;
       	<% end -%>
}

upstream  aacmi_core_authorize {
        <% @aacmi_auth_tomcat_servers.each do |aacmiauthtomcatservers| -%>
server  <%=aacmiauthtomcatservers-%>:8080  weight=1  max_fails=2  fail_timeout=5s;
        <% end -%>
}
