upstream  dns_<%=@customer1%>_core {
	<% @customer1_dns_servers.each do |dns1servers| -%>
server  <%=dns1servers-%>:80  weight=1  max_fails=2  fail_timeout=5s;
	<% end -%>
}

upstream  dns_<%=@customer2%>_core {
        <% @customer2_dns_servers.each do |dns2servers| -%>
server  <%=dns2servers-%>:80  weight=1  max_fails=2  fail_timeout=5s;
        <% end -%>
}
