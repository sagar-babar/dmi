upstream  uram_core {
       	<% @uram_servers.each do |uramservers| -%>
server  <%=uramservers-%>:8080  weight=1  max_fails=2  fail_timeout=5s;
       	<% end -%>
}

upstream  uram_ext {
       	<% @das_tomcat_servers.each do |dastomcatservers| -%>
server  <%=dastomcatservers-%>:8080  weight=1  max_fails=2  fail_timeout=5s;
       	<% end -%>
}
